<aura:component implements="force:lightningQuickActionWithoutHeader,force:appHostable,lightning:actionOverride,force:hasRecordId,force:hasSObjectName">
    <aura:attribute name="recordId" type="Id" />
    <aura:attribute name="sObjectName" type="String" />    
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <c:EnziUtils aura:id="utils"></c:EnziUtils>
    
    <div aura:id="component-body" class="component-body">
        {!v.body}
    </div>    
</aura:component>