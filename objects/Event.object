    <validationRules>
        <fullName>allow_copy_to_buildings_in_same_country</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
  Copy_to_Buildings_in_same_Country__c,
  NOT( LEFT(WhatId, 3) = "a00")
)</errorConditionFormula>
        <errorDisplayField>Copy_to_Buildings_in_same_Country__c</errorDisplayField>
        <errorMessage>Can't select "copy to buildings in same country" option if the event is not related to a building</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>edit_copy_to_buildings_in_same_country</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  ISCHANGED(Copy_to_Buildings_in_same_Country__c),  
  Copy_to_Buildings_in_same_Country__c,
  NOT(ISNEW()) 
)</errorConditionFormula>
        <errorDisplayField>Copy_to_Buildings_in_same_Country__c</errorDisplayField>
        <errorMessage>Can't edit "copy to buildings in same country" you need to create a new event</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Schedule_Tour</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <encodingKey>UTF-8</encodingKey>
        <linkType>url</linkType>
        <masterLabel>Schedule Tour</masterLabel>
        <openType>replace</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>/00U/e? 
who_id={!Opportunity.AccountId}&amp;
what_id={!Opportunity.Id}&amp;
evt2_lkid={!Opportunity.Primary_Member_Id__c}&amp;
evt2={!Opportunity.Primary_Member_Name__c}&amp;
evt5=Tour&amp;
retURL={!Opportunity.Id}</url>
    </webLinks>
